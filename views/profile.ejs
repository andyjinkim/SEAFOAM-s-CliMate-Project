


<div class="row">
        <div class="location-search">
            <form method="get" action="/profile" class="form-inline" >
                <input name="country" id="appendCountryUrl" type="text" placeholder="Country(if outside USA)">
                <input name="state" id="appendStateUrl" type="text" placeholder="State(USA only)">
                <input name="city" id="appendCityUrl" type="text" placeholder="City">
                <button type="button" class="btn btn-primary" id= "submit"> <i class="icon-search icon-white">Submit</i></button>
            </form>
        </div>
        <h4>Welcome back, <%= user.local.name || user.facebook.name %></h4>

        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
</div>

